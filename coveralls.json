{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "def.+(.+\/\/.+).+do"
  ],

  "custom_stop_words": [
  ],

  "coverage_options": {
    "minimum_coverage": 90,
    "output_dir": "cover/",
    "template_path": "test/coverage/templates/html/htmlcov/",
    "treat_no_relevant_lines_as_covered": true
  },

  "skip_files": [
    "test/",
    "lib/live_dj_web/views"
  ]
}