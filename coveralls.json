{
  "default_stop_words": [
    "apply",
    "defdata",
    "defimpl",
    "defmodule",
    "defrecord",
    "defstruct",
    "defsum",
    "def.+(.+\/\/.+).+do",
    "live_dashboard",
    "use"
  ],

  "custom_stop_words": [
    "Logger."
  ],

  "coverage_options": {
    "minimum_coverage": 91,
    "output_dir": "cover/",
    "template_path": "test/coverage/templates/html/htmlcov/",
    "treat_no_relevant_lines_as_covered": true
  },

  "skip_files": [
    "test/",
    "lib/live_dj_web/views"
  ]
}